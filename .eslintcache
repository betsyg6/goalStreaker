\n\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t</Grid>\n\t\t\t\t\t\t</Grid>\n\t\t\t\t\t\t<Button\n\t\t\t\t\t\t\tdisabled={isInvalid}\n\t\t\t\t\t\t\ttype='submit'\n\t\t\t\t\t\t\tfullWidth\n\t\t\t\t\t\t\tvariant='contained'\n\t\t\t\t\t\t\tcolor='primary'\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\tSign Up\n\t\t\t\t\t\t</Button>\n\t\t\t\t\t\t<Grid container justify='flex-end'>\n\t\t\t\t\t\t\t<Grid item>{error && <p>{error.message}</p>}</Grid>\n\t\t\t\t\t\t</Grid>\n\t\t\t\t\t</form>\n\t\t\t\t</div>\n\t\t\t</Container>\n\t\t);\n\t}\n}\n\nconst SignUpLink = () => (\n\t<p>\n\t\tDon't have an account? <Link to={ROUTES.SIGN_UP}>Sign Up</Link>\n\t</p>\n);\n\nconst SignUpForm = compose(withRouter, withFirebase)(SignUpFormBase);\nexport default SignUp;\nexport { SignUpForm, SignUpLink };\n","/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Landing/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/SignIn/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/PasswordForget/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Home/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Admin/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Account/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Firebase/index.js",[],"/Users/elizabethgroton/Desktop/Fullstack/fellowship/personalProjects/goalstreaker/src/components/Firebase/context.js",["100"],"import react, { Component } from 'react';\nconst FirebaseContext = react.createContext(null);\n\nexport const withFirebase = (Component) => (props) => (\n\t<FirebaseContext.Consumer>\n\t\t{(firebase) => <Component {...props} firebase={firebase} />}\n\t</FirebaseContext.Co